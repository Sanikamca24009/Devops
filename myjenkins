pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
  
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        
  
        stage('last') {
            steps {
             bat 'node hello'
                echo 'last'
            }
        }
    }
}
